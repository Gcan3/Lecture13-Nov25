#include <iostream>
#include <string>

using namespace std;
int main()
{
	string snack[3][4] = {
		{"Galaxy silk", "Mars Bar", "Snickers", "Bounty"},
		{"Flavoured Yougurt", "Oman chips", "Oreo", "Lays"},
		{"Apple", "Banana", "Orange", "Pear"}
	};

	for (int i = 0; i < 3; i++) {
		for (int j = 0; j < 4; j++) {
			cout << snack[i][j] << ", ";
		}
		cout << endl;
	}
}
